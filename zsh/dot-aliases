if type nvim > /dev/null; then
    alias k=kubectl
fi
if type nvim > /dev/null; then
    alias vim=nvim
fi
if type gdate > /dev/null; then
    alias date="gdate"
fi

alias gs="git status"
alias gf="git fetch"
alias gph="git push origin HEAD"
alias gnb="git checkout -b"
alias gl="git log --oneline"
alias gm="git commit -m"
alias gc="git checkout"
alias ga="git add"

alias lsf="ls -larth"
alias c="clear"
